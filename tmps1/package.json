{
  "name": "tmps2-typescript",
  "version": "1.0.0",
  "description": "Second lab for TMPS",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:factories": "node dist/factories/main.js",
    "start:notfactories": "node dist/notfactories/main.js",
    "dev": "ts-node src/index.ts",
