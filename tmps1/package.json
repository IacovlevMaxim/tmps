    "dev:notfactories": "ts-node src/notfactories/main.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}